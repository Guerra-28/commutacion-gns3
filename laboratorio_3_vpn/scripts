#Creacion de VPN

######### R2 #########################

#R2 configurar vpn
configure terminal
ip vrf UM
rd 1:1
route-target both 1:1
exit
interface fastEthernet 0/0
ip vrf forwarding UM
ip address 192.168.0.17 255.255.255.252
exit
exit
write


#R2 configurar bgp
configure terminal
router bgp 1
neighbor 8.8.8.8 remote-as 1
neighbor 8.8.8.8 update-source loopback 33
address-family vpnv4
neighbor 8.8.8.8 activate
exit
exit
exit


######################R2 Asignar OSPF entre los router PE y CE:
configure terminal
router ospf 2 vrf UM
network 192.168.0.0 0.0.0.255 area 0
exit
exit
write

#R10
configure terminal
router ospf 2
network 192.168.0.0 0.0.0.255 area 0
network 10.10.10.10 0.0.0.0 area 0
exit
exit
write


############################ configuracion ruta de distribucion

configure terminal
router bgp 1
address-family ipv4 vrf UM
redistribute ospf 2
exit
exit
exit
write

configure terminal 
router ospf 2
redistribute bgp 1  subnets
exit
exit
write


######### R2 #########################



######### R8 #########################


#R8 configuracion vpn
configure terminal
ip vrf UM
rd 1:1
route-target both 1:1
exit
interface fastEthernet 1/0
ip vrf forwarding UM
ip address 192.168.0.57 255.255.255.252
exit
exit
write




#R8 configurar bgp
configure terminal
router bgp 1
neighbor 2.2.2.2 remote-as 1
neighbor 2.2.2.2 update-source loopback 33
address-family vpnv4
neighbor 2.2.2.2 activate
exit
exit
exit




#R8 Asignar OSPF entre los router PE y CE:
configure terminal
router ospf 2 vrf UM
network 192.168.0.0 0.0.0.255 area 0
exit
exit
write

#R11
configure terminal
router ospf 2
network 192.168.0.0 0.0.0.255 area 0
network 11.11.11.11 0.0.0.0 area 0
exit
exit
write








############################ configuracion ruta de distribucion

configure terminal
router bgp 1
address-family ipv4 vrf UM
redistribute ospf 2
exit
exit
exit
write


######### R8 #########################
